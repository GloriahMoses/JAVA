class Main {
  public static void main(String[] args) {

    int number1 = 41;
    int number2 = 39;

    if(number1 < number2){
      System.out.println(number1 + " is less than " + number2);
    }
    else if(number1 == number2){
      System.out.println(number1 + " is equal " + number2);
    }
    else{
      System.out.println(number2 + " is less than " + number1);
    }
  }
}
